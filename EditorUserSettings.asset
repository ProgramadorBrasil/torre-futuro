%YAML 1.1
%TAG !u! tag:unity3d.com,2011:
--- !u!162 &1
EditorUserSettings:
  m_ObjectHideFlags: 0
  serializedVersion: 4
  m_ConfigSettings:
    # Otimizações críticas para performance do Asset Database
    vcSharedLogLevel:
      value: 0d5e400f0650
      flags: 0

    # Cache settings - CRÍTICO para performance
    UnityEditor.CacheServerMode:
      value: Local
      flags: 0

    # Asset Pipeline v2 settings
    UnityEditor.AssetPipelineMode:
      value: 2
      flags: 0

    # Parallel Import
    UnityEditor.ParallelImport:
      value: 1
      flags: 0

    # Async Shader Compilation - reduz travamentos
    UnityEditor.AsyncShaderCompilation:
      value: 1
      flags: 0

    # Directory Monitoring - desabilitar se causar problemas
    UnityEditor.DirectoryMonitoring:
      value: 1
      flags: 0

    # Refresh Mode - Auto pode causar lentidão
    UnityEditor.RefreshMode:
      value: Manual
      flags: 0

    # Asset Database v2
    UnityEditor.AssetDatabaseV2:
      value: 1
      flags: 0

    # Compress Assets on Import
    UnityEditor.CompressAssetsOnImport:
      value: 1
      flags: 0

    # Enter Play Mode Settings - acelera muito o teste
    UnityEditor.EnterPlayModeOptionsEnabled:
      value: 1
      flags: 0

    UnityEditor.EnterPlayModeOptions:
      value: 3
      flags: 0

    # Cache Server Settings
    UnityEditor.CacheServerEnabled:
      value: 1
      flags: 0

    UnityEditor.CacheServerEndpoint:
      value: 127.0.0.1:10080
      flags: 0

    UnityEditor.CacheServerNamespacePrefix:
      value: default
      flags: 0

    UnityEditor.CacheServerEnableDownload:
      value: 1
      flags: 0

    UnityEditor.CacheServerEnableUpload:
      value: 1
      flags: 0

    # Max Concurrent Imports
    UnityEditor.MaxConcurrentImports:
      value: 4
      flags: 0

    # Import Worker Count
    UnityEditor.ImportWorkerCount:
      value: 4
      flags: 0

    # Desired Import Worker Count
    UnityEditor.DesiredImportWorkerCount:
      value: 4
      flags: 0

    # Script Compilation
    UnityEditor.ScriptCompilationDuringPlay:
      value: 0
      flags: 0

    # Auto Refresh
    UnityEditor.AutoRefresh:
      value: 0
      flags: 0

    # Show All Files in Project
    UnityEditor.ShowAllFilesInProject:
      value: 0
      flags: 0

    # Precompiled References
    UnityEditor.UsePrecompiledReferences:
      value: 1
      flags: 0

    # Asset Store Cache Path
    UnityEditor.AssetStoreCachePath:
      value: D:\UnityCache\AssetStore
      flags: 0

    # GI Cache Path
    UnityEditor.GICachePath:
      value: D:\UnityCache\GICache
      flags: 0

  m_VCAutomaticAdd: 0
  m_VCDebugCom: 0
  m_VCDebugCmd: 0
  m_VCDebugOut: 0
  m_SemanticMergeMode: 2
  m_VCShowFailedCheckout: 1
  m_VCOverwriteFailedCheckoutAssets: 1
  m_VCOverlayIcons: 1
  m_VCAllowAsyncUpdate: 1